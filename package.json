{
  "name": "switchreducer",
  "version": "1.0.0",
  "description": "A small but useful redux switch case helper",
  "license": "MIT",
  "author": "te schultz",
  "main": "dist/switchreducer.js",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/artisin/switchreducer"
  },
  "bugs": {
    "url": "https://github.com/artisin/switchreducer/issues"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "yarn run clean && env-cmd production webpack --color --progress",
    "clean": "rm -rf dist/*",
    "commit": "git cz",
    "dev": "yarn run clean && env-cmd development webpack -w",
    "prepare": "npm test",
    "prepublishOnly": "yarn run build && yarn run test",
    "release:major": "yarn run test && bumped release major",
    "release:minor": "yarn run test && bumped release minor",
    "release:patch": "yarn run test && bumped release patch",
    "test": "yarn run build && env-cmd test mocha  --compilers js:babel-core/register __tests__/tests.run.js",
    "test:debug": "iron-node node_modules/mocha/bin/_mocha --compilers js:babel-core/register __tests__/tests.run.js",
    "test:build": "yarn install"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "bumped": "^0.10.4",
    "bumped-changelog": "^0.3.10",
    "bumped-terminal": "^0.7.3",
    "commitizen": "^2.9.6",
    "cz-customizable-te": "^6.0.0",
    "env-cmd": "7.0.0",
    "eslint": "^4.3.0",
    "eslint-plugin-lodash": "2.4.5",
    "eslint-plugin-node": "5.2.0",
    "fs-extra": "^4.0.1",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "redux": "^3.7.2",
    "should": "^13.1.0",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable-te"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  }
}
